{
  "name": "bits",
  "version": "1.0.0",
  "description": "A starter template with expressjs & winston & typescript.",
  "main": "src/build/server.js",
  "scripts": {
    "build": "npx tsc -b src",
    "start": "node src/build/server.js",
    "dev": "concurrently \"npx tsc -p src/tsconfig.json -w\" \"nodemon -q src/build/server.js\""
  },
  "keywords": [
    "api",
    "cors",
    "dotenv",
    "expressjs",
    "helmet",
    "typescript",
    "winston"
  ],
  "author": "Refaat Bitar",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dayjs": "^1.11.6",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^6.0.0",
    "lodash": "^4.17.21",
    "morgan": "1.10.0",
    "winston": "3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/lodash": "^4.14.186",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.11.4",
    "concurrently": "^7.5.0",
    "nodemon": "^2.0.20",
    "typescript": "^4.8.4"
  }
}
