{
  "name": "bits",
  "version": "1.0.0",
  "description": "A starter template with expressjs & winston & typescript.",
  "main": "src/build/server.js",
  "scripts": {
    "build": "npx tsc -b src",
    "start": "node src/build/server.js",
    "dev": "concurrently \"npx tsc -p src/tsconfig.json -w\" \"nodemon -q src/build/server.js\""
  },
  "keywords": [
    "api",
    "cors",
    "dotenv",
    "expressjs",
    "helmet",
    "typescript",
    "winston"
  ],
  "author": "Refaat Bitar",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "helmet": "^8.0.0",
    "lodash": "^4.17.21",
    "morgan": "1.10.0",
    "winston": "3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/lodash": "^4.17.16",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.13.10",
    "concurrently": "^9.1.2",
    "nodemon": "^3.1.9",
    "typescript": "^5.8.2"
  }
}
